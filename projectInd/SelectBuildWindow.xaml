<Window x:Class="projectInd.SelectBuildWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projectInd"
        mc:Ignorable="d"
        Title="SelectBuildWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="{Binding Title}" FontWeight="Bold" Margin="0 0 0 5"/>
            <ComboBox x:Name="BuildComboBox" DisplayMemberPath="Name" Margin="0 0 0 10"/>
            <Button Content="OK" Click="Ok_Click" Width="60" HorizontalAlignment="Right"/>
        </StackPanel>
    </Grid>
</Window>
